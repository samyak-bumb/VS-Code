      "\tfont-family: \"Baloo 2\", cursive\n}\n:root{\n\t--light: #fbfbfb;\n\t--dark: #010101;\n\t--selection: #5773ff;\n}",
      "::selection{",
      "\tcolor: var(--light);",
      "\tbackground-color: var(--selection)\n}",
      /*
      Light Green (White BG *REQUIRED*): a7f5a9
      Light Crimson-Pink: ea4c89
      Light Blue: a8cefc, dbeafe
      Light Green: 00d1b2
      Light Cyan: 9bf0f0
      Dark Purple-Blue: 5773ff
      */
      "body{",
      "\tmin-height: 100vh;",
      "\tcolor: var(--light);",
      "\tbackground-color: var(--dark)",
      "}\n\n.${0:class}{\n\t\n}"
    ]
  },
